    "express": "^4.16.4",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.4",
    "node": "^13.10.1",
    "react-bootstrap": "^1.0.0",
    "react-collapsible": "^2.7.0",
    "react-dom": "^16.13.0",
    "react-items-carousel": "^2.8.0",
    "request": "^2.88.2",
    "underscore": "^1.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/helper-plugin-utils": "^7.8.3",
